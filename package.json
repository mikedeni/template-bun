{
  "name": "template-bun",
  "description": "A Phaser 3 template using bun.",
  "version": "1.0.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phaserjs/template-bun.git"
  },
  "author": "Phaser Studio <support@phaser.io> (https://phaser.io/)",
  "license": "MIT",
  "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
  "bugs": {
    "url": "https://github.com/phaserjs/template-bun/issues"
  },
  "homepage": "https://github.com/phaserjs/template-bun#readme",
  "scripts": {
    "dev": "bun log.js dev && bunx --bun vite --config vite/config.dev.mjs",
    "build": "bun log.js build && bunx --bun vite build --config vite/config.prod.mjs",
    "dev-nolog": "bunx --bun vite --config vite/config.dev.mjs",
    "build-nolog": "bunx --bun vite build --config vite/config.prod.mjs"
  },
  "devDependencies": {
    "terser": "^5.37.0",
    "typescript": "^5.7.3",
    "vite": "^5.4.14"
  },
  "dependencies": {
    "phaser": "^3.87.0"
  }
}